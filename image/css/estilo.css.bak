/* fuentes personalizadas*/
@font-face{
	font-family: titulos;
	src: url('fuentes/28 Days Later.ttf') format('truetype');
}
@font-face{
	font-family: parrafos;
	src: url('fuentes/Eleggari.ttf') format('truetype');
}
@font-face{
	font-family: vinculos;
	src: url('fuentes/Elegant.ttf') format('truetype');
}

/* estilos basicos */
h1,h2,h3,h4,h5,h6{
	font-family: titulos;
	text-shadow:2px 2px 5px rgba(0,0,0,0.5); /* Para poner sombra al texto: primer valor distancia X, segundo valor distancia Y, tercer valor difuminado de la sombra por ultimo el color de la sombra*/
}
h1{
	transform:translate(150px 100px);
	-moz-transform:translate(150px 100px);
	-webkit-transform:translate(150px 100px);
	position:relative;
	color:White;
	font-size: 37px;
	top:-20%;
	margin-top:100px;
	letter-spacing:0px;
	display:inline;
	
}

h2{
	transform:translate(150px 100px);
	-moz-transform:translate(150px 100px);
	-webkit-transform:translate(150px 100px);
	position:relative;
	color:White;
	font-size: 25px;
	top:-20%;
	margin-top:100px;
	letter-spacing:0px;
	display:inline;
	
}

p{
	font-family: parrafos;
	text-shadow:1px 1px 3px rgba(0,0,0,0.5); /* Para poner sombra al texto*/
}
/*Maquetacion principal*/
body{
	text-align: center;
	margin:0px;
	background-color:white;
	background: url('../img/reja1.png'),url('../img/negro.png'),url('../img/azul3-2.png');
}

#tipo{
position:absolute;
transform:translate(100px, 10px);
-webkit-transform:translate(100px, 10px);
display:inline-table;
}

#principal{
	text-align: left;
	margin: auto;
	width:80%;
	height:100%;
	position: relative;
	background: rgba(255,255,255,0.6);
	border-radius: 5px 35px 5px 35px;
	-moz-border-radius: 5px 35px 5px 35px;
	-webkit-border-radius: 5px 35px 5px 35px;
	
	box-shadow: 0px 0px 10px white;
	-moz-box-shadow: 0px 0px 10px white;
	-webkit-box-shadow: 0px 0px 10px white;
}

#cajaheader{
width:100%;
height:100px;
position: absolute;
background: rgba(190,205,255,0.5);
}

header{
	display:table;
	margin:auto;
	text-align:left;
	width:100%;
	height:100px;
	border: 2px solid white;
	border-radius: 5px 35px 0px 0px;
	-moz-border-radius: 5px 35px 0px 0px;
	-webkit-border-radius: 5px 35px 0px 0px;
	
	box-shadow: 0px 0px 10px white;
	-moz-box-shadow: 0px 0px 10px white;
	-webkit-box-shadow: 0px 0px 10px white;
}

#logo{
	width:100px;
	height:100px;
	display:inline-table;
	position:relative;
}
#milogo{
	width:90px;
	height:90px;
	font-family:parrafos;
	font-size:20px;
	text-align:left;
	color: white;
	
	background:url('../img/logo.gif');
	background-size:100% 100%;
	background-repeat:no-repeat;
	display:box;
	
	border:1px solid white;
	border-radius: 5px 20px 0px 20px;
	-moz-border-radius: 5px 20px 0px 20px;
	-webkit-border-radius: 5px 20px 0px 20px;
	
	box-shadow: 0px 0px 10px white;
	-moz-box-shadow: 0px 0px 10px white;
	-webkit-box-shadow: 0px 0px 10px white;
	
	float:left;

	animation: parpadeo 5s;
	-webkit-animation: parpadeo 5s;
	-moz-animation: parpadeo 5s;
	animation-iteration-count: infinite;
	-moz-animation-iteration-count: infinite;
	-webkit-animation-iteration-count: infinite;
	
}

/* Maquetando la navegacion*/
nav{
	display:inline;
	float:right;
	margin:20px;	
}
nav ul{
	font-size:16px;
	font-family:parrafos;
	padding-left:20px;
	margin-left:0px;
	background-color:grey;
	color: white;
	width:50%;
	position:relative;
	display:inline;
}

nav ul li{
		display:inline;
		text-align:center;
}

nav ul li a{
	background-color:rgba(255,255,255,0.5);
	color:white;
	text-decoration:none;
	float: right;
	border: 1px solid white;
	border-radius: 0px 10px 0px 10px;
	-moz-border-radius: 0px 10px 0px 10px;
	-webkit-border-radius: 0px 10px 0px 10px;
	box-shadow:0px 0px 5px white;
	-moz-box-shadow:0px 0px 5px white;
	-webkit-box-shadow:0px 0px 5px white;
	width:80px;
}

nav ul li a:hover{
	background-color:white;
	color:black;	
	box-shadow:0px 0px 25px white;
	-moz-box-shadow:0px 0px 25px white;
	-webkit-box-shadow:0px 0px 25px white;
}

canvas{
	border: 0px solid white;	
}

footer{
	display:table;
	margin:auto;
	text-align:center;
	width:100%;
	background: darkgray;	
		border: 2px solid white;
	border-radius: 0px 0px 5px 35px;
	-moz-border-radius: 0px 0px 5px 35px;
	-webkit-border-radius: 0px 0px 5px 35px;
	
	box-shadow: 0px 0px 10px white;
	-moz-box-shadow: 0px 0px 10px white;
	-webkit-box-shadow: 0px 0px 10px white;
}

/*Maquetacion del contenido */

#contenidos{
width:100%;
	border-top: 0px solid white;
   border-right: 2px solid white;   
   border-bottom: 2px solid white;
   border-left: 2px solid white;

box-shadow: 0px 0px 10px white;
-moz-box-shadow: 0px 0px 10px white;
-webkit-box-shadow: 0px 0px 10px white;
-o-box-shadow: 0px 0px 10px white;
background: rgba(255,255,255,0.5);
}

article{
	width:75%; 
	background:rgba(255,255,255,0);
	display:table;
	float:right;
	margin:10px;
	height:100%;
	border:1px solid white;
	border-radius: 0px 25px 0px 25px;
	-moz-border-radius: 0px 25px 0px 25px;
	-webkit-border-radius: 0px 25px 0px 25px;
	
	box-shadow: 3px 3px 10px black;
	-moz-box-shadow: 3px 3px 10px black;
	-webkit-box-shadow: 3px 3px 10px black;
	padding:10px 10px 10px 150px;	
	
	opacity:0.5;
	transition:opacity 1s, box-shadow 1s,transform 1s, background 1s;
	-moz-transition:opacity 1s, -moz-box-shadow 1s,-moz-transform 1s, background 1s;
	-webkit-transition:opacity 1s, -webkit-box-shadow 1s,-webkit-transform 1s, background 1s;	
	
	transform:scale(1,1);
	-moz-transform:scale(1,1);
	-webkit-transform:scale(1,1);
}
article:hover{
	box-shadow: 0px 0px 25px white;
	-moz-box-shadow: 0px 0px 25px white;
	-webkit-box-shadow: 0px 0px 25px white;
	opacity:1;
	transform:scale(1.1,1.1);
	-moz-transform:scale(1.1,1.1);
	-webkit-transform:scale(1.1,1.1);
	background:rgba(255,255,255,1);
}


/*Maquetando el contenido tangencial*/
aside{
float: left;
display: inline-table;
width:20%;
background: rgba(255,255,255,0.5);

margin:10px;
padding:10px;
border: 1px solid white;
border-radius: 0px 25px 0px 25px;
-webkit-border-radius: 0px 25px 0px 25px;
box-shadow: 3px 3px 10px black;
-webkit-box-shadow: 3px 3px 10px black;
text-decoration:none;
-webkit-transition: -webkit-box-shadow 1s, background 1s;
}

aside:hover{
box-shadow: 0px 0px 25px white;
-webkit-box-shadow: 0px 0px 25px white;
background: rgba(255,255,255,1);
}

aside ul{
width: 100%;
height: 100%;
padding-left:0;
padding-right:0;
color:white;
float:left;
font-family:parrafos;
font-size: 14px;
text-decoration:none;
list-style: none;
}

aside ul li{
width:100%;
text-decoration:none;
border: 1px solid white;
border-radius: 0px 5px 0px 5px;
-webkit-border-radius: 0px 5px 0px 5px;
text-decoration:none;
}

aside ul li a{
width:100%;
height:30px;
overflow:hidden;
text-decoration:none;
}

#sobremi{
	width:100%;
	height:120px;
	background:black;	
	text-decoration:none;
	color:white;
	text-align:justify;
	padding:10px;
	font-size:12px;
	display:table;
	overflow:hidden;	
}

@keyframes parpadeo{
	0%{box-shadow: 0px 0px 10px white;}
	50%{box-shadow: 0px 0px 50px white;}
	100%{box-shadow: 0px 0px 10px white;}	
}
@-webkit-keyframes parpadeo{
	0%{-webkit-box-shadow: 0px 0px 10px white;}
	50%{-webkit-box-shadow: 0px 0px 50px white;}
	100%{-webkit-box-shadow: 0px 0px 10px white;}	
}
@-moz-keyframes parpadeo{
	0%{-moz-box-shadow: 0px 0px 10px white;}
	50%{-moz-box-shadow: 0px 0px 50px white;}
	100%{-moz-box-shadow: 0px 0px 10px white;}	
}

#mifoto{
	width:75px;
	height:80px;
	font-family:parrafos;
	font-size:20px;
	text-align:left;
	color: white;
	
	background:url('../img/josetapia.jpg');
